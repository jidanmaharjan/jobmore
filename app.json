{
  "expo": {
    "scheme": "caltra",
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      {
        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
        "recordAudioAndroid": true
      }
    ],
    "name": "Caltra",
    "slug": "caltra",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "jsEngine": "jsc",
      "package": "com.jyamaru.caltra",
      "bundleIdentifier": "com.jyamaru.caltra"
    }
  }
}
